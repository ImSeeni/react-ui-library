{
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false,
    "importOrder": [
        "react",
        "^@mui",
        "^@storybook",
        "<THIRD_PARTY_MODULES>",
        "revedia-common",
        "^@/types",
        "^@/theme",
        "^@/providers",
        "^@/components",
        "^@/utilities",
        "^@/styles",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": false,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
